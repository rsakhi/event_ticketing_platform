<%= render "shared/header" %>
<div>
  <h3>Event Details</h3>
  <ul>
    <li>Title: <%= @event.title%></li>
    <li>Date: <%= @event.start_date.strftime("%F")%></li>
    <li>Price: <%= @event.price%></li>
    <li>Venue: <%= @event.venue%></li>
    <li>Totle Number of geuest: <%= @event.users.count %></li>
  </ul>
  <div>Guest List </div>
  <% @event.users.each do |user| %>
    <ul>
      <li><%=user.name%>
    </ul>
  <% end%>
</div>
